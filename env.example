# Voice Roleplay System Environment Variables
# Copy this file to .env and fill in your values

# HuggingFace Hub Token (required for speaker diarization)
# Get your token from: https://huggingface.co/settings/tokens
HUGGINGFACE_HUB_TOKEN=

# AI Service API Keys
GROQ_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=

# Voice Services
VOICEVOX_HOST=localhost
VOICEVOX_PORT=50021

# Advanced Features
ENABLE_VOICE_CLONING=false
ENABLE_SPEAKER_DIARIZATION=false
ENABLE_ADVANCED_VOICE_ANALYSIS=true

# Logging
LOG_LEVEL=INFO

# Development
DEBUG=false
RELOAD=true 